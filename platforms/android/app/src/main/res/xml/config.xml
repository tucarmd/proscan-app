<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="7" id="com.carmd.proscan2" ios-CFBundleVersion="1" version="1.0.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
