{
  "name": "com.carmd.proscan2",
  "version": "1.0.6",
  "author": "Viet Huynh",
  "homepage": "https://carmd.com",
  "scripts": {
    "setup-android-env": "nvm use 16 && export JAVA_HOME=/Library/Java/JavaVirtualMachines/zulu-11.jdk/Contents/Home",
    "rename-aab": "mv platforms/android/app/build/outputs/bundle/release/app-release.aab platforms/android/app/build/outputs/bundle/release/ProScan2.aab",
    "rename-apk": "mv platforms/android/app/build/outputs/apk/release/app-release.apk platforms/android/app/build/outputs/apk/release/ProScan2.apk",
    "clean": "./platforms/android/gradlew -p platforms/android clean",
    "clean-prepare": "rm -rf www && ./platforms/android/gradlew -p platforms/android clean && ionic cordova prepare android",
    "android-build-stag-apk": "rm -rf www && ionic cordova build android --configuration=staging --release -- -- --packageType=\"apk\"",
    "android-build-prod-aab": "rm -rf www && ionic cordova build android --prod --release -- -- --packageType=bundle && npm run rename-aab",
    "android-build-prod-apk": "rm -rf www && ionic cordova build android --prod --release -- -- --packageType=\"apk\" && npm run rename-apk",
    "ios-prepare": "ionic cordova prepare ios",
    "ios-prepare-stag": "rm -rf www && ionic cordova prepare ios --configuration=staging --release",
    "ios-prepare-prod": "rm -rf www && ionic cordova prepare ios --prod --release",
    "android-run": "ionic cordova run android",
    "android-run-live": "ionic cordova run android -l",
    "open-xcode": "open platforms/ios/ProScan 2.0.xcworkspace",
    "log-cat": "adb logcat --pid=$(adb shell pidof -s com.carmd.proscan2)",
    "add-ble": "ionic cordova plugin add projects/cordova-plugins/cordova-plugin-ble-central --variable BLUETOOTH_USAGE_DESCRIPTION=\"This app uses Bluetooth to connect to Pro15 tools.\" --variable IOS_INIT_ON_LOAD=false"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
